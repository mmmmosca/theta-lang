# TM simulator using Python-side tm blueprint
# transitions: [state, symbol, new_symbol, dir, next_state]
let tape = [[1;1;1;0;0;0]]
let head = 0
let state = 'q0'

# run and print
let result = tm.run([], tape, head, state, 100)
io.out("tm result tape:", result[0])
io.out("tm result head:", result[1])
io.out("tm result state:", result[2])
